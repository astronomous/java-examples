<?xml version="1.0" encoding="US-ASCII"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <profiles>
        <profile>
            <id>stream-machine</id>
            <repositories>
                <repository>
                    <id>stream-machine-snapshot</id>
                    <url>artifactregistry://europe-west4-maven.pkg.dev/stream-machine-279011/snapshot</url>
                </repository>
                <repository>
                    <id>stream-machine-release</id>
                    <url>artifactregistry://europe-west4-maven.pkg.dev/stream-machine-279011/release</url>
                </repository>
            </repositories>
            <properties>
                <altSnapshotDeploymentRepository>stream-machine-snapshot::default::artifactregistry://europe-west4-maven.pkg.dev/stream-machine-279011/snapshot</altSnapshotDeploymentRepository>
                <altReleaseDeploymentRepository>stream-machine-release::default::artifactregistry://europe-west4-maven.pkg.dev/stream-machine-279011/release</altReleaseDeploymentRepository>
            </properties>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>stream-machine</activeProfile>
    </activeProfiles>

</settings>
